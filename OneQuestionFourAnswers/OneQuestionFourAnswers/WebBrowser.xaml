<Window x:Class="OneQuestionFourAnswers.WebBrowser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OneQuestionFourAnswers"
        mc:Ignorable="d"
        Title="WebBrowser" ResizeMode="NoResize" Height="600" Width="700" Loaded="OnWindowLoaded" WindowStartupLocation="CenterOwner">
    <Grid>
        <WebBrowser x:Name="WebView" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Navigated="OnWebViewNavigated"/>
    </Grid>
</Window>
